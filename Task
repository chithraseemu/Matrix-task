first_multiple_input=input().rstrip().split()
n=int(first_multiple_input[0])
m=int(first_multiple_input[1])
matrix=[]
for _ in range(n):
    matrix_item=input()
    matrix.append(matrix_item)
output=""
for i in range(m):
    for j in range(n):
        char=matrix[j][i]
        if(char.isalnum() or char.isspace()):
            output=output+char
        else:
            output=output+" "
print(output)
         
